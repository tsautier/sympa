msgstr "fjärrlösenord"

#: src/lib/Sympa/Config/Schema.pm:3113 src/lib/Sympa/Config/Schema.pm:3296
msgid "idle timeout"
msgstr "tidsgräns för inaktivitet"

#: src/lib/Sympa/Config/Schema.pm:3122 src/lib/Sympa/Config/Schema.pm:3306
#: src/lib/Sympa/Config/Schema.pm:3402 src/lib/Sympa/Config/Schema.pm:3593
#: src/lib/Sympa/Config/Schema.pm:3998 src/lib/Sympa/Config/Schema.pm:4184
